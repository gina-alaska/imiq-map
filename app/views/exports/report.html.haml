%h2 Exports reported from #{@start_date} to #{@end_date}.

%h3= "Number of exports: #{@export_count}"
%h3= "Number of new users registered: #{@new_user_count}"
%h3= "Total number of users who created an export: #{@user_names.length}"
%h3 Users:
- @user_names.each do |name|
  %ul
    %li= "#{name}" 
    %ul
      %li= "Email: #{name.email}"
      %li= "Total number exports: #{name.exports.length}"
      %li= "Report period number exports: #{name.exports.where(created_at: @start_date...@end_date).length}"
