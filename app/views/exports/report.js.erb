var modal = $("<%= escape_javascript render('/exports/report_modal.html') %>").modal()
modal.on('turbolinks:load loaded.bs.modal', function() {
  $('.date-editor').datetimepicker({
    format: 'YYYY/MM/DD'
  });
});

modal.on('hidden.bs.modal', function() {
  $(this).remove();
  $('.datetimepicker').remove()
});