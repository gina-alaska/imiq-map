<!DOCTYPE html>
<html>
<head>
  <title>Imiq Map</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <section id="content">
    <%= yield %>
  </section>
  <section id="footer">
    <%= render 'layouts/footer' %>
  </section>
  <div class="spinner hidden">
    <div class="rect1"></div>
    <div class="rect2"></div>
    <div class="rect3"></div>
    <div class="rect4"></div>
    <div class="rect5"></div>
  </div>
</body>
</html>
